{
    "files.associations": {
      "*.sql": "jinja-sql",
      "*.yml": "jinja-yaml"
    },
    "files.exclude": {
      "**/__pycache__": true,
      "**/.pybuilder": true,
      "**/.venv": true,
      "**/.mypy_cache": true,
      "**/.coverage": true,
      "**/.cache": true,
      "**/.pytest_cache": true,
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[jinja-sql]": {
      "editor.defaultFormatter": null, // use run-on-save
      "editor.formatOnSave": false // use run-on-save
    },
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true
    },
    "git.openRepositoryInParentFolders": "always",
    "editor.formatOnSave": true,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "notebook.insertFinalNewline": true,
    "[yaml]": {
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "cSpell.customDictionaries": {
      "custom-dictionary-workspace": {
        "name": "custom-dictionary-workspace",
        "path": "${workspaceFolder:starr-oncology-data-lake-arpah}/.cspell/custom-dictionary-workspace.txt",
        "addWords": true,
        "scope": "workspace"
      }
    },
    "[quarto]": {
      "editor.defaultFormatter": "quarto.quarto"
    },
  }
